package scraper;

public class Wish {
	String gameid;
	String oprice;
	String sprice;
	String discount;
	String price;
	boolean discounted;
	String imgurl;
	String rank;
	String name;
	String dateAdded;
	String username;
	
	public Wish(String gameid, String oprice, String sprice, String discount,
			String price, boolean discounted, String imgurl, String rank,
			String name, String dateAdded, String username) {
		super();
		this.gameid = gameid;
		this.oprice = oprice;
		this.sprice = sprice;
		this.discount = discount;
		this.price = price;
		this.discounted = discounted;
		this.imgurl = imgurl;
		this.rank = rank;
		this.name = name;
		this.dateAdded = dateAdded;
		this.username = username;
	}
	/**
	 * @return the gameid
	 */
	public String getGameid() {
		return gameid;
	}
	/**
	 * @param gameid the gameid to set
	 */
	public void setGameid(String gameid) {
		this.gameid = gameid;
	}
	/**
	 * @return the oprice
	 */
	public String getOprice() {
		return oprice;
	}
	/**
	 * @param oprice the oprice to set
	 */
	public void setOprice(String oprice) {
		this.oprice = oprice;
	}
	/**
	 * @return the sprice
	 */
	public String getSprice() {
		return sprice;
	}
	/**
	 * @param sprice the sprice to set
	 */
	public void setSprice(String sprice) {
		this.sprice = sprice;
	}
	/**
	 * @return the discount
	 */
	public String getDiscount() {
		return discount;
	}
	/**
	 * @param discount the discount to set
	 */
	public void setDiscount(String discount) {
		this.discount = discount;
	}
	/**
	 * @return the price
	 */
	public String getPrice() {
		return price;
	}
	/**
	 * @param price the price to set
	 */
	public void setPrice(String price) {
		this.price = price;
	}
	/**
	 * @return the discounted
	 */
	public boolean isDiscounted() {
		return discounted;
	}
	/**
	 * @param discounted the discounted to set
	 */
	public void setDiscounted(boolean discounted) {
		this.discounted = discounted;
	}
	/**
	 * @return the imgurl
	 */
	public String getImgurl() {
		return imgurl;
	}
	/**
	 * @param imgurl the imgurl to set
	 */
	public void setImgurl(String imgurl) {
		this.imgurl = imgurl;
	}
	/**
	 * @return the rank
	 */
	public String getRank() {
		return rank;
	}
	/**
	 * @param rank the rank to set
	 */
	public void setRank(String rank) {
		this.rank = rank;
	}
	/**
	 * @return the name
	 */
	public String getName() {
		return name;
	}
	/**
	 * @param name the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}
	/**
	 * @return the dateAdded
	 */
	public String getDateAdded() {
		return dateAdded;
	}
	/**
	 * @param dateAdded the dateAdded to set
	 */
	public void setDateAdded(String dateAdded) {
		this.dateAdded = dateAdded;
	}
	
	
}

